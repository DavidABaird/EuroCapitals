<script type="text/javascript">
    var allWeatherString = "@ViewBag.allWeather".replace(/&quot;/g, "\"");
    console.log(allWeatherString);
    var allWeather = JSON.parse(allWeatherString);
    allWeather = allWeather.weather;
</script>
@{
    ViewBag.Title = "Weather";
}
@Styles.Render("~/content/bootstrap.min.css")
@Styles.Render("~/content/Site.css")
@Scripts.Render("~/scripts/jquery-1.10.2.min.js")
@Scripts.Render("~/scripts/list.js")
@Scripts.Render("~/scripts/weather_page.js")
<h3 class ="header-title">EuroCap Weather</h3>
<div id="WeatherInstances" class="annotated-list">

    <input class="search" placeholder="Search" />
    <button class="sort" data-sort="fetchTimeStamp">Date</button>
    <button class="sort" data-sort="cityName">City</button>
    <button class="sort" data-sort="temperature">Temperature</button>
    <button class="sort" data-sort="windspeed">Wind</button>

    <ul class="list">
    </ul>

</div>